{
  "name": "portfolio",
  "version": "0.0.0",
  "description": "Portfolio Cardstack application",
  "repository": "https://github.com/cardstack/portfolio",
  "author": "Hassan Abdel-Rahman <hassan.abdelrhaman@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 8"
  },
  "devDependencies": {
    "@cardstack/eslint-config": "^0.12.0",
    "@cardstack/test-support": "^0.12.18",
    "eslint": "^5.9.0",
    "eslint-plugin-ember": "^6.0.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^8.0.0",
    "glob": "^7.1.1",
    "mocha": "^4.0.1",
    "require-uncached": "^1.0.3",
    "resolve": "^1.3.3"
  },
  "scripts": {
    "start-prereqs": "docker run -d -p 5432:5432 --rm --name cardstack-pg cardstack/pg-test",
    "stop-prereqs": "docker stop cardstack-pg",
    "start": "cd portfolio && node ./node_modules/.bin/ember serve",
    "start-hub": "cd portfolio && ember hub:start",
    "start-ember": "cd portfolio && HUB_URL=http://localhost:3000 node ./node_modules/.bin/ember s",
    "test": "mocha ./bin/run.js --timeout 10000 && node ./node_modules/@cardstack/test-support/ember-test-runner.js cards && node ./node_modules/@cardstack/test-support/ember-test-runner.js packages && cd portfolio && node ./node_modules/.bin/ember test",
    "card-tests": "node ./node_modules/@cardstack/test-support/ember-test-runner.js cards",
    "node-tests": "mocha ./bin/run.js --timeout 10000",
    "lint": "eslint . --cache"
  },
  "dependencies": {},
  "workspaces": [
    "cards/*",
    "packages/*",
    "portfolio"
  ],
  "private": true
}
