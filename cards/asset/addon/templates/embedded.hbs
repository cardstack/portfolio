<a class="asset-embedded asset-embedded--{{title}}" href={{cardstack-url content}}>
  <header class="asset-embedded__header">
    <div class="asset-embedded__title-row">
      {{svg-jar content.logo class="asset-embedded__logo"}}
      <h3 class="asset-embedded__title" data-test-asset-embedded-title>
        {{content.title}}
      </h3>
    </div>
    <a class="asset-embedded__tag" href="/">Savings</a>
  </header>
  <section class="asset-embedded__section">
    <div class="asset-embedded__data-row">
      <span class="asset-embedded__value">2.1434</span>
      <span class="asset-embedded__unit" data-test-asset-embedded-unit>{{content.unit}}</span>
      {{!-- Add arrow icon and modal --}}
    </div>
    <div>
      <span class="asset-embedded__value--secondary">$8509.30</span>
      <span>USD</span>
    </div>
  </section>
  <footer class="asset-embedded__footer">
    <div class="asset-embedded__address">1Hzk...iY36v</div>
    <div class="asset-embedded__last-active">Last active 3m ago</div>
  </footer>
</a>
